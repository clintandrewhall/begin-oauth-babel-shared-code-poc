{
  "name": "begin-oauth-babel-shared-code-poc",
  "version": "0.0.0",
  "description": "Let's explore shared code using the OAuth example, as a PoC",
  "repository": "https://github.com/clintandrewhall/begin-oauth-babel-shared-code-poc",
  "license": "MIT",
  "scripts": {
    "lint": "eslint lib --ext ts --ignore-pattern node_modules --fix",
    "clean": "rm -rf src && mkdir src",
    "install": "node scripts/install",
    "webpack": "npx webpack",
    "build": "npm run clean && npm run install && npm run webpack",
    "watch": "npm run build -- --watch",
    "start": "concurrently \"npm run watch\" \"npx sandbox\"",
    "deploy": "npm run build && npx deploy"
  },
  "devDependencies": {
    "@architect/architect": "latest",
    "@architect/sandbox": "latest",
    "@architect/functions": "latest",
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-typescript": "^7.9.0",
    "@types/node": "^12.12.31",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-root-import": "^5.1.0",
    "babel-polyfill": "^6.26.0",
    "bufferutil": "^4.0.1",
    "concurrently": "^5.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "core-js": "^3.6.4",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "fork-ts-checker-webpack-plugin": "^4.1.2",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3",
    "utf-8-validate": "^5.0.2",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "minimist": ">=1.2.3",
    "tiny-json-http": "^7.1.2"
  }
}
